model IPInstance {
    id           Int            @unique
    name         String
    hierarchy    String
    architecture Architecture[]
    model        IPModel
    modelId      Int
    modelVersion String
    parent       IPInstance?
    parentId     Int?
    children     IPInstance[]
    bindings     Json // IPInstanceBindingData in data-structure.ts
    data         Json // IPInstanceData in data-structure.ts
    memo         String
    createdAt    DateTime
    updatedAt    DateTime
}

model IPModel {
    id           Int          @unique
    type         String
    isSystem     Boolean
    isComposite  Boolean
    isCustom     Boolean
    data         Json // IPModelData in data-structure.ts
    ownerGroup   UserGroup
    ownerGroupId Int
    instances    IPInstance[]
    deleteFlag   Boolean
    createdAt    DateTime
    updatedAt    DateTime
}
